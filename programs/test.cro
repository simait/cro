module test {

	class root() {

		switch = new switch();

		method init() {
			send switch.enable();
			switch.disable();
			switch.enable();
			switch.disable();
		}

	}

	class switch() {
		var enabled := false;

		method enable() {
			enabled := true;
		}

		method disable() {
			if (not enabled) {
				send notification("Double disable!\r\n");
			}
			enabled := false;
		}

		method notification(msg) {
			print(msg);
		}
	}

}
