module test {
	class root() {
		switch = new switch();
		method init():void {
			switch.enable();
			send switch.disable();
			switch.enable();
			switch.disable();
			switch.enable();
		}
	}

	class switch() {
		var enabled:bool := false;

		method enable():void {
			enabled := true;
		}

		method disable():void {
			enabled := false;
		}
	}
}