module test {
	class root() {
		switch = new switch();
		method init():void {
			send switch.disable();
			switch.disable();
			switch.enable();
			switch.disable();
			switch.enable();
		}
	}
	class switch() {
		var enabled:bool := false;

		method enable():void {
			enabled := true;
		}

		method disable():void {
			if (not enabled) {
				send notification("Double disable!\r\n");
			}
			enabled := false;
		}

		method notification(msg:string):void {
			print(msg);
		}
	}
}